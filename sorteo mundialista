fibonacci=[]
x=0
y=1
num = int(input())
for n in range(num):
    fibonacci.append(x+y)
    aux = x + y
    x = y
    y = aux
print(fibonacci)



fibonacci=[]
x=0
y=1
z = int(input())
for n in range(z):
    fibonacci.append(x+y)
    aux = x + y
    x = y
    y = aux
print(fibonacci[z-3])
}
