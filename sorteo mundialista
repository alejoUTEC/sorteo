A=int(input("ingrese primer valor "))
B=int(input("ingrese segundo valor "))
C=int(input("ingrese tercer valor "))
if A<B and A<C:
     print("el menor munero es",A)
elif B<A and B<C:
     print("el menor munero es",B)
elif C<A and C<B:
     print("el menor munero es",C)



n=float(input("ingrese nota "))
if 18<=n<=20:
    print("excelencia")
if 15<=n<=17:
    print("distincion")
if 11<=n<=14:
    print("aprobado")
if n<11:
    print("desaprobado")







