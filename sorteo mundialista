c=int(input("Por favor ingrese numero de clientes: "))
lista=[]
i=0
while i<c:
    n=input("nombre: ")
    lista.insert(i,n)
    i=i+1
lista.pop(0)
print(lista)
lista.pop()
print(lista)
lista.insert(0,"primera")
lista.insert(1,"segunda")
lista.append("penultima")
lista.append("ultima")
print(lista)








